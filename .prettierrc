{
  "printWidth": 180,
  "tabWidth": 2,
  "useTabs": false,
  "semicolons": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "crlf",
  "singleQuote": true,
  "singleAttributePerLine": false,
  "quoteProps": "preserve"
}